use umm::python::{new_project, new_query_grader, show_results};

pub async fn main() {
    let project = new_project()?;
    // No .constraint() call - should default to MustMatchAtLeastOnce
    let grade = new_query_grader()
        .project(project)
        .file("comprehensive")
        .req_name("test")
        .out_of(5.0)
        .uses_for_loop()
        .run()
        .await?;
    show_results([grade])?;
    Ok(())
}
