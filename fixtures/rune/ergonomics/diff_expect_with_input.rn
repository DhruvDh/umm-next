use umm::python::{new_project, new_diff_grader, show_results};

pub async fn main() {
    let project = new_project()?;
    let diff = new_diff_grader()
        .project(project)
        .file("main")
        .req_name("test")
        .out_of(5.0)
        .expect_with_input("ping\n", "ping")
        .run()
        .await?;
    show_results([diff])?;
    Ok(())
}
