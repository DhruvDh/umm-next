use umm::python::{new_project, new_query_grader, show_results};

pub async fn main() {
    let project = new_project()?;
    let grade = new_query_grader()
        .project(project)
        .file("comprehensive")
        .req_name("test")
        .out_of(5.0)
        .must_not_use_recursion("non_recursive_sum")
        .run()
        .await?;
    show_results([grade])?;
    Ok(())
}
